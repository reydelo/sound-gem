<header>
  <h3> Friends (<%= @users.count %>) </h3>
</header>
<ul class="img-list">
  <% @users.each do |user| %>
  <li>
  <a><%= link_to (image_tag(user.avatar_url, class: "avatar")), user_path(user) %>
  <span class="text-content"><span><%= link_to user.soundcloud_username, user_path(user) %></span></span>
  </a>
  </li>
  <% end %>
</ul>
